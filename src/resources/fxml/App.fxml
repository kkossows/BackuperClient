                          <Label fx:id="lb_filePath" layoutX="119.0" layoutY="38.0" text="Label" />
                          <Label fx:id="lb_progressType" layoutX="27.0" layoutY="14.0" text="Restoring version:" />
                          <ProgressBar fx:id="progressBar" layoutX="119.0" layoutY="14.0" prefHeight="17.0" prefWidth="205.0" progress="0.0" />
                          <ProgressIndicator fx:id="progressIndicator" layoutX="335.0" layoutY="13.0" prefHeight="37.0" prefWidth="27.0" progress="0.0" />
                          <Label fx:id="lb_copying" layoutX="27.0" layoutY="39.0" text="Copying:" />
                       </children>
                    </AnchorPane>
                 </bottom>
              </BorderPane>
           </children>
        </AnchorPane>
    </children>
